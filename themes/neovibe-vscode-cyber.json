    "tokenColors": [
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#FF00FF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#FFD75F"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#FF00FF"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#808CA6",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#FFD75F"
            }
        },
        {
            "scope": "type",
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "foreground": "#FF00FF"
            }
        },
        {
            "scope": "operator",
            "settings": {
                "foreground": "#00F5FF"
            }
        }
    ]
}