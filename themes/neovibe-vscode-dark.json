  "tokenColors": [
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#00D9FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#FF6AC1"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#39FF14"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7777AA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF4500"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "operator",
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#FF6AC1"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#FF6AC1"
      }
    },
    {
      "scope": "entity.name.namespace",
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#39FF14"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FF6AC1"
      }
    },
    {
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FF3C38",
        "fontStyle": "bold underline"
      }
    }
  ]
}