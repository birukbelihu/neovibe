    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 3,
    "editor.cursorSurroundingLines": 2,
    "editor.cursorSurroundingLinesStyle": "all",
    "editorBracketMatch.background": "#FF6AC1AA",
