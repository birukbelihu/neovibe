{
  "name": "NeoVibe Light (LED Neon)",
  "type": "light",
  "colors": {
    "editor.background": "#fafafa",
    "editor.foreground": "#111111",
    "editorCursor.foreground": "#ff007f",
    "editor.lineHighlightBackground": "#f0f0f0",
    "editor.selectionBackground": "#00ffff33",
    "editor.inactiveSelectionBackground": "#e8faff",
    "activityBar.background": "#f5f5f5",
    "activityBar.foreground": "#111111",
    "activityBar.activeBorder": "#ff007f",
    "activityBarBadge.background": "#ff007f",
    "activityBarBadge.foreground": "#ffffff",
    "sideBar.background": "#fdfdfd",
    "sideBar.foreground": "#222222",
    "statusBar.background": "#f9f9f9",
    "statusBar.foreground": "#111111",
    "titleBar.activeBackground": "#fdfdfd",
    "titleBar.activeForeground": "#111111",
    "input.background": "#f5f0f5",
    "input.foreground": "#7c00ff",
    "input.border": "#7c00ff",
    "input.focusBorder": "#ff007f",
    "button.background": "#f5f0f5",
    "button.foreground": "#7c00ff",
    "button.hoverBackground": "#ff007f33",
    "button.secondaryBackground": "#fafafa",
    "button.secondaryHoverBackground": "#ff007f33",
    "textLink.foreground": "#7c00ff",
    "textLink.activeForeground": "#ff007f",
    "progressBar.background": "#ff007f",
    "dropdown.listHoverBackground": "#ff007f33",
    "dropdown.listForeground": "#111111",
    "dropdown.listBackground": "#fdfdfd",
    "dropdown.foreground": "#7c00ff",
    "inputOption.activeBackground": "#ff007f33",
    "inputOption.activeForeground": "#ff007f",
    "inputOption.activeBorder": "#ff007f",
    "inputValidation.errorBackground": "#fff0f0",
    "inputValidation.errorBorder": "#ff1744",
    "inputValidation.warningBackground": "#fff8f0",
    "inputValidation.warningBorder": "#ff9100",
    "inputValidation.infoBackground": "#f0ffff",
    "inputValidation.infoBorder": "#00c8a0"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#888888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#7c00ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#00c8a0"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ff006e"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#0077ff"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#aa00ff"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#ff1744"
      }
    },
    {
      "scope": "number",
      "settings": {
        "foreground": "#ff9100"
      }
    }
  ]
}