    "tokenColors": [
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#FF5F7F",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#FFD75F"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#FFB84F"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#FF927F"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#FF3CA6"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#B088A3",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#FFB84F"
            }
        },
        {
            "scope": "type",
            "settings": {
                "foreground": "#FF5F7F"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#FF3CA6"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "foreground": "#FFD75F"
            }
        },
        {
            "scope": "operator",
            "settings": {
                "foreground": "#FF5F7F"
            }
        }
    ]
}